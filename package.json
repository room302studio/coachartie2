{
  "name": "coachartie2",
  "version": "1.1.0",
  "private": true,
  "description": "Coach Artie monorepo - AI assistant with multiple communication interfaces",
  "author": "Room 302 Studio",
  "license": "CC-BY-NC-4.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "node scripts/smart-dev.js",
    "dev:force": "turbo run dev",
    "dev:pm2": "pm2 start ecosystem.config.js",
    "dev:docker": "docker-compose -f docker/docker-compose.yml up",
    "stop": "pm2 stop all",
    "restart": "pm2 restart all",
    "logs": "pm2 logs",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\"",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepare": "husky install",
    "test:queue": "tsx scripts/test-queue-flow.ts",
    "test:all": "tsx scripts/test-all-services.ts",
    "prompt:tui": "DATABASE_PATH=./data/coachartie.db tsx tools/prompt-tui.ts",
    "prompt:cli": "DATABASE_PATH=./data/coachartie.db tsx tools/prompt-cli.ts",
    "prompts": "npm run prompt:tui",
    "version:patch": "npm version patch && git push --follow-tags",
    "version:minor": "npm version minor && git push --follow-tags",
    "version:major": "npm version major && git push --follow-tags"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "concurrently": "^9.2.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.2.0",
    "husky": "^8.0.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsx": "^4.7.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.14.0",
  "dependencies": {
    "blessed": "^0.1.81",
    "csv-parser": "^3.2.0",
    "nodemailer": "^6.9.8"
  },
  "pnpm": {
    "overrides": {
      "static-eval@<=2.0.1": ">=2.0.2",
      "static-eval@<2.0.0": ">=2.0.0",
      "minimist@<0.2.1": ">=0.2.1",
      "minimist@<0.2.4": ">=0.2.4",
      "esbuild@<=0.24.2": ">=0.25.0",
      "@nuxtjs/mdc@<=0.13.2": ">=0.13.3",
      "@nuxtjs/mdc@<0.17.2": ">=0.17.2",
      "form-data@>=4.0.0 <4.0.4": ">=4.0.4",
      "@eslint/plugin-kit@<0.3.4": ">=0.3.4",
      "vite@<=5.4.19": ">=5.4.20",
      "axios@>=1.0.0 <1.12.0": ">=1.12.0",
      "tar-fs@>=2.0.0 <2.1.4": ">=2.1.4",
      "nodemailer@<7.0.7": ">=7.0.7",
      "vite@>=5.2.6 <=5.4.20": ">=5.4.21",
      "koa@>=2.16.2 <2.16.3": ">=2.16.3"
    }
  }
}
