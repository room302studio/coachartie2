{
  "name": "coachartie2",
  "version": "1.0.0",
  "private": true,
  "description": "Coach Artie monorepo - AI assistant with multiple communication interfaces",
  "author": "Room 302 Studio",
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:pm2": "pm2 start ecosystem.config.js",
    "dev:docker": "docker-compose -f docker/docker-compose.yml up",
    "stop": "pm2 stop all",
    "restart": "pm2 restart all",
    "logs": "pm2 logs",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepare": "husky install",
    "test:queue": "tsx scripts/test-queue-flow.ts",
    "test:all": "tsx scripts/test-all-services.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "concurrently": "^9.2.0",
    "globals": "^16.2.0",
    "husky": "^8.0.3",
    "nodemon": "^3.1.10",
    "tsx": "^4.7.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.14.0"
}
