{
  "name": "@coachartie/capabilities",
  "version": "1.0.0",
  "type": "module",
  "description": "Core capabilities service for Coach Artie - processes messages and executes AI capabilities",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "dev:tsx": "tsx watch src/index.ts",
    "dev:concurrent": "concurrently \"tsx watch src/index.ts\"",
    "start": "node dist/index.js",
    "mcp": "tsx src/mcp-index.ts",
    "mcp:dev": "tsx watch src/mcp-index.ts",
    "test": "vitest --config ../../vitest.config.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@coachartie/shared": "workspace:*",
    "@fastify/cors": "^11.0.1",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@modelcontextprotocol/server-brave-search": "^0.6.2",
    "@mozilla/readability": "^0.6.0",
    "@shelm/wikipedia-mcp-server": "^1.0.1",
    "@supabase/supabase-js": "^2.49.4",
    "@types/cors": "^2.8.19",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "axios": "^1.9.0",
    "bullmq": "^5.1.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "^9.29.0",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.5.1",
    "fastify": "^5.4.0",
    "helmet": "^7.1.0",
    "html-entities": "^2.6.0",
    "mathjs": "^14.0.0",
    "neverthrow": "^6.1.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.91.1",
    "pino-pretty": "^13.0.0",
    "uuid": "^11.1.0",
    "wikipedia-mcp-server": "^0.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/node-fetch": "^2.6.12",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
