{
  "name": "coach-artie-capabilities",
  "version": "1.0.0",
  "description": "Coach Artie's Capability Workshop - A service for building, managing and executing capabilities for AI assistants",
  "main": "dist/src/app.js",
  "scripts": {
    "start": "node dist/src/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "build:fast": "node build-no-check.js",
    "test": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "test:unit": "vitest run --coverage src",
    "test:integration": "vitest run --coverage tests/integration",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"tests/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"tests/**/*.{ts,js}\"",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc src --out docs/api --excludePrivate --excludeInternal",
    "docs:watch": "typedoc src --out docs/api --excludePrivate --excludeInternal --watch",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "sync-db-types": "node scripts/sync-db-types.js",
    "validate-db-types": "node scripts/validate-db-types.js"
  },
  "keywords": [
    "AI",
    "capabilities",
    "LLM",
    "API"
  ],
  "author": "Coach Artie Team",
  "license": "MIT",
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "@supabase/supabase-js": "^2.49.4",
    "axios": "^1.9.0",
    "better-sqlite3": "^11.10.0",
    "bottleneck": "^2.19.5",
    "chance": "^1.1.12",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "discord.js": "^14.18.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "html-entities": "^2.6.0",
    "neverthrow": "^6.1.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.91.1",
    "puppeteer": "^24.6.0",
    "sqlite-vec": "^0.1.7-alpha.2",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "winston-loki": "^6.0.8",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/node-fetch": "^2.6.12",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.56.0",
    "glob": "^11.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.28.5",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
