# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here

# File paths (optional - defaults provided)
DISCORD_STATUS_FILE=/app/data/discord-status.json
DISCORD_METRICS_FILE=/app/data/discord-metrics.json
DISCORD_EVENTS_FILE=/app/data/discord-events.json

# Health check server port
HEALTH_PORT=3001

# Capabilities service endpoint
CAPABILITIES_BASE_URL=http://localhost:18239

# Redis configuration (for BullMQ)
REDIS_URL=redis://localhost:6379

# Log level
LOG_LEVEL=info