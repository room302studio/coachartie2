{
  "name": "@ejfox/vue-tsne",
  "version": "1.0.0",
  "description": "A Vue 3 composable for t-SNE dimensionality reduction visualization",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "vue3",
    "composable",
    "tsne",
    "t-sne",
    "dimensionality-reduction",
    "visualization",
    "machine-learning",
    "embeddings"
  ],
  "author": "EJ Fox",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ejfox/coachartie2.git",
    "directory": "packages/vue-tsne"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "@vueuse/core": "^10.0.0"
  },
  "dependencies": {
    "tsne-js": "^1.0.3"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vue": "^3.4.0",
    "@vueuse/core": "^10.9.0"
  }
}